{% set selected = 'logs' %}
{% extends 'templates/base.twig' %}
{% block icon %}fa-book{% endblock %}
{% block title %}Logs{% endblock %}
{% block head_title %}Logs{% endblock %}



{% block main_content %}

<div style="height:15px"></div>
<table class="sharif_table">
	<thead>
		<tr>
			<th>#</th>
			<th>Login ID</th>
			<th>Username</th>
      <th>IP Address</th>
      <th>Last Login</th>
      <th>Last 24 Hour Login ID</th>
		</tr>
	</thead>
	{% for log in logs %}
		<tr>
			<td>{{ loop.index }}</td>
			<td id='{{ log.login_id }}'>{{ log.login_id }}</td>
			<td>{{ log.username }}</td>
      <td>{{ log.ip_address }}</td>
      <td>{{ log.timestamp }}</td>
      <td><a href="#{{ log.last_24h_login_id }}">{{ log.last_24h_login_id }}</a></td>
		</tr>
	{% endfor %}
</table>
{% endblock %} {# main_content #}
